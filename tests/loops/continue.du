/**
* continue.du
*
* Testing continue in a loop
*/

var x = [];

for (var i = 0; i < 10; i += 1) {
    if (i % 2 == 0)
        continue;

    x.push(i);
}

assert(x == [1, 3, 5, 7, 9]);

var i = 0;
var y = [];

while (i < 10) {
    i += 1;
    if (i % 2 == 0)
        continue;

    y.push(i);
}

assert(y == [1, 3, 5, 7, 9]);

var z = [];

for (var i = 0; i < 5; i += 1) {
    for (var j = 0; j < 5; j += 1) {
        if (j % 2 == 0)
            continue;

        z.push(j);
    }
    if (i % 2 == 0)
        continue;

    z.push(i);
}

assert(z == [1, 3, 1, 3, 1, 1, 3, 1, 3, 3, 1, 3]);
